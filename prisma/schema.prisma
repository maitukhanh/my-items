// This is your Prisma schema file
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Item {
  id        String   @id @default(uuid())
  name      String
  location  String
  imageUrl  String?
  createdAt DateTime @default(now())
}
